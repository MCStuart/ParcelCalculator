@using ParcelShop;

<h1>Your Parcel</h1>
<ul>

@if (Model.Count == 0)
{
  <p>You have no parcels to ship!</p>
}
@if(Model.Count != 0)
{
  @foreach (Parcel parcel in Model)
  {
    <li>Volume of your parcel: @parcel.Volume() in^3
    <br>Cost to ship it: $@parcel.CostToShip()</li>
  }
}

</ul>
<a href="/parcels/new">Add a new parcel</a>
